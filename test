class CoffeeMaker {
    init(coffee) {
        this.coffee = coffee;
    }

    brew() {
        print "Enjoy your " + this.coffee;
        this.coffee = nil;
    }
}

var coffeeMaker = CoffeeMaker("latte");
coffeeMaker.brew();

class Oops {
  init() {
    fun f() {
      print "not a method";
    }

    this.field = f;
  }
}

var oops = Oops();
oops.field();